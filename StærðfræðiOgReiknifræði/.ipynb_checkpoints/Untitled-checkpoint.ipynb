    "import math\n",
    "\n",
    "def fall(n,a):\n",
    "    if a > 1: \n",
    "        return n*a\n",
    "    else:\n",
    "        return math.sqrt(a**(2*n)+1)\n",
    "print(f\"Útkoman er {fall(2,3)} og {fall(3,-2)}\")"
